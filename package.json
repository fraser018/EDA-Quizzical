{
  "name": "boilerplate-react-webpack",
  "version": "0.2.2",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "dev": "run-p watch:client watch:server",
    "watch:client": "webpack --watch",
    "watch:server": "nodemon --watch server",
    "start": "node server",
    "test": "jest --watchAll",
    "postinstall": "webpack",
    "h:deploy": "git push heroku heroku:master",
    "h:migrate": "heroku run npx knex migrate:latest",
    "h:rollback": "heroku run npx knex migrate:rollback",
    "h:seed": "heroku run npx knex seed:run"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/client/setup.js"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "chartist": "^0.11.4",
    "express": "^4.17.1",
    "fs": "0.0.2",
    "he": "^1.2.0",
    "knex": "^0.20.4",
    "nosleep.js": "^0.9.0",
    "npm-check-updates": "^3.2.2",
    "npm-run-all": "^4.1.5",
    "pg": "^7.14.0",
    "react": "^16.12.0",
    "react-chartist": "^0.14.2",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "socket.io": "^2.4.0",
    "superagent": "^5.1.2",
    "uifx": "^2.0.6",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.3",
    "sqlite3": "^4.1.1",
    "save": "^2.4.0",
    "supertest": "^4.0.2"
  }
}
